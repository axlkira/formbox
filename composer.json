{
    "name": "formbox/formbuilder",
    "description": "Aplicación web para generación y gestión de formularios drag & drop.",
    "type": "project",
    "require": {
        "php": ">=8.0",
        "laravel/framework": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
